@{
    ViewBag.Title = "Registration";
    Layout = "~/Views/Shared/_LayoutRegistration.cshtml";
}
@using (Html.BeginForm("Registration", "Registration", FormMethod.Post, new { id = "FormId" }))
{
    @Html.AntiForgeryToken()
}
<div data-loading></div>
<div class="container" style="padding-bottom: 50px">
    <div class="title">
        <h2 style="text-align: center">Mukhyamantri Krushi Udyoga Yojana</h2>
    </div>
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <h3 style="text-align: center">Registration</h3>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="well form-horizontal" ng-show="Question">
        <div class="well" style="padding-top:35px">
            <div class="form-group" ng-show="compRegd">
                <div class="text-right col-md-7 col-sm-7 col-xs-12" style="padding-top: 6px"><b>Have you attempted / completed Registration?</b></div>
                <div class="text-left col-md-5 col-sm-5 col-xs-12">
                    <a style="color: black" href="#" data-toggle="tooltip" title="Click here for new registration" data-placement="top"><label class="radio-inline"><input type="radio" name="refnochk" ng-model="CkeckRefno" value="No" ng-click="CheckNo()" />No</label></a>
                    <a style="color: black" href="#" data-toggle="tooltip" title="Click here if you have a Reference Number" data-placement="top"><label class="radio-inline"><input type="radio" name="refnochk" ng-model="CkeckRefno" value="Yes" ng-click="CheckYes()" />Yes</label></a>
                </div>
            </div>
            <form name="chkregister" ng-submit="chkregister.$valid" class="form-horizontal" ng-show="CkeckRefno" autocomplete="off" novalidate>
                <div class="form-group">
                    <div class="text-right col-md-6 col-sm-6 col-xs-12" style="padding-top: 6px"><b>Expansion Project</b></div>
                    <div class="text-left col-md-6 col-sm-6 col-xs-12">
                        <label class="radio-inline"><input type="radio" name="expProj" ng-model="ExpProj" value="No" ng-click="ExpNo()" />No</label>
                        <label class="radio-inline"><input type="radio" name="expProj" ng-model="ExpProj" value="Yes" ng-click="ExpYes()" />Yes</label>
                    </div>
                </div>
                <div ng-show="RegdAdr">
                    <div class="form-group">
                        <div class="col-md-2 col-sm-2 col-xs-12"></div>
                        <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px">Reference No <span style="color: red" class="required">*</span></div>
                        <div class="col-md-5 col-sm-5 col-xs-12">
                            <input type="text" capitalize reference-no placeholder="Enter Reference No." name="referenceno" ng-model="PutReferenceNumber" class="form-control" required maxlength="25" />
                            <div class="has-error" ng-show="chkregister.$submitted || chkregister.referenceno.$touched" style="color:orangered">
                                <span ng-show="chkregister.referenceno.$error.required">This is a required field </span>
                                <span ng-show="chkregister.referenceno.$error.ser">Invalid ReferenceNumber</span>
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    </div>
                    <div class="form-group">
                        <div class="col-md-2 col-sm-2 col-xs-12"></div>
                        <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px">Aadhaar No <span style="color: red" class="required">*</span></div>
                        <div class="col-md-5 col-sm-5 col-xs-12">
                            <input type="text" id="aid" placeholder="Enter Aadhaar No." name="addharno" ng-model="txtAadharNo" numbers-only maxlength="12" required="required" class="form-control">
                            <div class="has-error" ng-show="chkregister.$submitted || chkregister.addharno.$touched" style="color:orangered">
                                <span ng-show="chkregister.addharno.$error.required">This is a required field.</span>
                                <span ng-show="chkregister.addharno.$error.ser">Invalid Aadhaar No.</span>
                            </div>
                        </div>
                        <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    </div>
                    <div class="form-group">
                        <div class="text-center col-md-12 col-sm-12 col-xs-12">
                            <input type="submit" id="chkdtls" value="Continue to Registration" class="btn btn-primary" ng-click="GEtAllBYRefno()" />
                        </div>
                    </div>
                </div>
            </form>
            <form name="chkExpansion" ng-submit="chkExpansion.$valid" class="form-horizontal" ng-show="CheckExpansion" autocomplete="off" novalidate>
                <div class="form-group">
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px">Go-Ahead No. <span style="color: red" class="required">*</span></div>
                    <div class="col-md-5 col-sm-5 col-xs-12">
                        <input type="text" placeholder="Enter Go-Ahead No." name="goAheadNo" ng-model="GoAheadNo" class="form-control" required maxlength="25" />
                        <div class="has-error" ng-show="chkExpansion.$submitted || chkExpansion.goAheadNo.$touched" style="color:orangered">
                            <span ng-show="chkExpansion.goAheadNo.$error.required">This is a required field </span>
                            <span ng-show="chkExpansion.goAheadNo.$error.ser">Invalid ReferenceNumber</span>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                </div>
                <div class="form-group">
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px">Aadhaar No <span style="color: red" class="required">*</span></div>
                    <div class="col-md-5 col-sm-5 col-xs-12">
                        <input type="text" id="aid" placeholder="Enter Aadhaar No." name="addharno" ng-model="txtAadharNo" numbers-only maxlength="12" required="required" class="form-control">
                        <div class="has-error" ng-show="chkExpansion.$submitted || chkExpansion.addharno.$touched" style="color:orangered">
                            <span ng-show="chkExpansion.addharno.$error.required">This is a required field.</span>
                            <span ng-show="chkExpansion.addharno.$error.ser">Invalid Aadhaar No.</span>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                </div>
                <div class="form-group">
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                    <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px">Farmer ID <span style="color: red" class="required">*</span></div>
                    <div class="col-md-5 col-sm-5 col-xs-12">
                        <input type="text" placeholder="Enter Farmer ID" name="farmerID" ng-model="txtFarmerID" maxlength="12" required="required" class="form-control">
                        <div class="has-error" ng-show="chkExpansion.$submitted || chkExpansion.farmerID.$touched" style="color:orangered">
                            <span ng-show="chkExpansion.farmerID.$error.required">This is a required field.</span>
                            <span ng-show="chkExpansion.farmerID.$error.ser">Invalid Aadhaar No.</span>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-2 col-xs-12"></div>
                </div>
                <div class="form-group">
                    <div class="text-center col-md-12 col-sm-12 col-xs-12">
                        <input type="submit" id="chkdtls" value="Continue to Expansion" class="btn btn-primary" ng-click="" />
                    </div>
                </div>
            </form>
        </div>
        <div class="text-right col-md-12 col-sm-12 col-xs-12" style="margin-top: -18px">
            @Html.ActionLink("Back to Prerequisites", "Prerequisites", "Prerequisites", null, new { @class = "btn btn-success", @type = "submit" })
        </div>
    </div>
    <div class="well form-horizontal" ng-show="MainPage" style="margin-bottom: 75px">
        <div class="row" style="margin-top: 0px">
            <div class="wizard">
                <div class="wizard-inner">
                    <div class="connecting-line"></div>
                    <ul class="nav nav-tabs" role="tablist">
                        <li role="presentation" class="active">
                            <a href="#step1" data-toggle="tab" aria-controls="step1" role="tab" title="Applicant Details">
                                <span class="round-tab">
                                    <i class="glyphicon glyphicon-user"></i>
                                </span>
                            </a>
                            <h6 style="text-align:center"><b>Applicant Details</b></h6>
                        </li>
                        <li role="presentation" class="disabled" id="normal">
                            <a href="#step2" data-toggle="tab" aria-controls="step2" role="tab" title="Project Information">
                                <span class="round-tab">
                                    <i class="glyphicon glyphicon-tasks"></i>
                                </span>
                            </a>
                            <h6 style="text-align:center"><b>Project Information</b></h6>
                        </li>
                        <li role="presentation" class="disabled">
                            <a href="#step3" data-toggle="tab" aria-controls="step3" role="tab" title="Proposed Location of CAE with Land Particulars">
                                <span class="round-tab">
                                    <i class="glyphicon glyphicon-paperclip"></i>
                                </span>
                            </a>
                            <h6 style="text-align:center"><b>Proposed Location of CAE with Land Particulars</b></h6>
                        </li>
                        <li role="presentation" class="disabled">
                            <a href="#step4" data-toggle="tab" aria-controls="step4" role="tab" title="Documents to be Uploaded">
                                <span class="round-tab">
                                    <i class="glyphicon glyphicon-picture"></i>
                                </span>
                            </a>
                            <h6 style="text-align:center"><b>Documents to be Uploaded</b></h6>
                        </li>
                        <li role="presentation" class="disabled">
                            <a href="#step5" data-toggle="tab" aria-controls="step5" role="tab" title="Preview & Submit">
                                <span class="round-tab">
                                    <i class="glyphicon glyphicon-save"></i>
                                </span>
                            </a>
                            <h6 style="text-align: center"><b>Preview & Submit</b></h6>
                        </li>
                    </ul>
                </div>
                <div class="tab-content">
                    <div class="tab-pane active" role="tabpanel" id="step1" style="padding-bottom: 30px">
                        <form class="form-horizontal" name="regform" role="form" ng-show="FirstInput" autocomplete="off" novalidate>
                            <div class="row container">
                                <div class="col-md-2 col-sm-2 col-xs-12"></div>
                                <div class="col-md-8 col-sm-8 col-xs-12">
                                    <div class="form-group">
                                        <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                            Applicant Type
                                            <span style="color: red" class="required">*</span>
                                        </label>
                                        <div class="radio col-md-3 col-sm-3 col-xs-12">
                                            <label>
                                                <input type="radio" name="ApplicantType" ng-model="rbApplicantType" value="Individual" ng-click="emptyField()" onclick="alert('Please choose the Applicant Type carefully. You will not be able to change it after the generation of Reference Number.');" required /><text>Individual</text>
                                            </label>
                                        </div>
                                        <div class="radio col-md-4 col-sm-4 col-xs-12">
                                            <label>
                                                <input type="radio" name="ApplicantType" ng-model="rbApplicantType" value="Group" ng-click="hideFields()" onclick="alert('Please choose the Applicant Type carefully. You will not be able to change it after the generation of Reference Number.');" required /><text>Group</text>
                                            </label>
                                        </div>
                                        <span style="color: red" ng-show="regform.ApplicantType.$touched && regform.ApplicantType.$invalid && regform.ApplicantType.$error.required">This is a required field.</span>
                                    </div>
                                    <div class="form-group" ng-show="rbApplicantType=='Group' || rbApplicantType=='Individual'">
                                        <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                            Farmer ID
                                            <span style="color: red" class="required">*</span>
                                        </label>
                                        <div ng-show="inputFarmerID">
                                            <div class="col-md-4 col-sm-4 col-xs-12 inputGroupContainer">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                                    <input type="text" alpha-numeric name="fid" id="farmerID" ng-model="txtFarmerID" placeholder="Enter the Farmer ID" class="form-control" required style="text-transform: uppercase" maxlength="15" />
                                                </div>
                                                <span style="color: red" ng-show="regform.fid.$touched && regform.fid.$invalid && regform.fid.$error.required">This is a required field.</span>
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-12">
                                                <button type="button" class="btn btn-primary col-md-12 col-sm-12 col-xs-12" ng-click="showRelationType()">Proceed</button>
                                            </div>
                                        </div>
                                        <div ng-show="labelFarmerID">
                                            <div class="col-md-4 col-sm-4 col-xs-12 inputGroupContainer">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                                    <input type="text" maxlength="15" ng-model="txtFarmerID" class="form-control" style="text-transform: uppercase" disabled />
                                                </div>
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-12">
                                                <button type="button" class="btn btn-warning col-md-12 col-sm-12 col-xs-12" ng-click="resetFarmerID()">Reset</button>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group" ng-show="rTShow">
                                        <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                            Relation Type (with Farmer ID)
                                            <span style="color: red" class="required">*</span>
                                        </label>
                                        <div class="radio col-md-3 col-sm-3 col-xs-12">
                                            <label>
                                                <input type="radio" name="RelationType" ng-model="rbRelationType" value="Self" ng-click="validateFarmerID()" onclick="alert('Please choose the Relation Type carefully. You will not be able to change it after the generation of Reference Number.');" required /><text>Self</text>
                                            </label>
                                        </div>
                                        <div class="radio col-md-4 col-sm-4 col-xs-12">
                                            <label>
                                                <input type="radio" name="RelationType" ng-model="rbRelationType" value="Relative" ng-click="SHRF()" onclick="alert('Please choose the Relation Type carefully. You will not be able to change it after the generation of Reference Number.');" required /><text>Relative</text>
                                            </label>
                                        </div>
                                        <span style="color: red" ng-show="regform.RelationType.$touched && regform.RelationType.$invalid && regform.RelationType.$error.required">This is a required field.</span>
                                    </div>
                                    <div class="form-group" ng-show="relationFields">
                                        <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                            Relation with Farmer ID
                                            <span style="color: red" class="required">*</span>
                                        </label>
                                        <div class="col-md-7 col-sm-7 col-xs-12 inputGroupContainer">
                                            <div class="input-group">
                                                <span class="input-group-addon"><i class="glyphicon glyphicon-education"></i></span>
                                                <select class="form-control" name="relationFID" ng-model="ddlRelationFID" ng-options="i.INTRELATION as i.VCHORIYAREL for i in fidRelationship" ng-change="validateFarmerID()" required>
                                                    <option disabled value="">---Select Your Relation with Farmer ID---</option>
                                                </select>
                                            </div>
                                            <span style="color: red" ng-show="regform.relationFID.$touched && regform.relationFID.$invalid && regform.relationFID.$error.required">This is a required field.</span>
                                        </div>
                                    </div>
                                    <div ng-show="FarmerDetails">
                                        <div class="form-group">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Name of the Applicant
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-4 col-sm-4 col-xs-12" style="padding-top: 5px" ng-show="relSin">
                                                {{lblApplicantName}}
                                            </div>
                                            <div class="col-md-3 col-sm-3 col-xs-12" style="padding-top: 5px; text-align: right" ng-show="relationMul.length >= 2 && rbRelationType == 'Relative' && relSin == true && relMul == false">
                                                <input type="button" value="Change Applicant Name" class="btn btn-xs btn-warning" ng-click="changeAN()" />
                                            </div>
                                            <div class="col-md-7 col-sm-7 col-xs-12 inputGroupContainer" ng-show="relMul">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-education"></i></span>
                                                    <select class="form-control" name="RelationName" ng-model="ddlRelationName" ng-options="i for i in relationMul" ng-change="confirmRelation(ddlRelationName)" required>
                                                        <option disabled value="">---Select the Applicant's Name---</option>
                                                    </select>
                                                </div>
                                                <span style="color: red" ng-show="regform.RelationName.$touched && regform.RelationName.$invalid && regform.RelationName.$error.required">This is a required field.</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Name of the Father / Husband
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px">
                                                {{t.FatherHusbandName}}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Gender
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px">
                                                {{t.GenderName}}
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Category
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px">
                                                {{t.CategoryName}}
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="relationSelf">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Voter ID Card No.
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px">
                                                {{t.VoterIDCardNo}}
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="relationSelf">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Aadhar ID Card No.
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px">
                                                {{t.AadharIDCardNo}}
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="relationFields">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Relative's Voter ID Card No.
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12 inputGroupContainer">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-education"></i></span>
                                                    <input type="text" placeholder="Enter your Voter ID Card No." name="rVoterID" alpha-numeric capitalize class="form-control" ng-model="txtRVoterID" maxlength="15" required />
                                                </div>
                                                <span style="color: red" ng-show="regform.rVoterID.$touched && regform.rVoterID.$invalid && regform.rVoterID.$error.required">This is a required field.</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="relationFields">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Relative's Aadhaar Card No.
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12 inputGroupContainer">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-education"></i></span>
                                                    <input type="text" placeholder="Enter your Aadhaar Card No." name="rAadhaarNo" numbers-only capitalize class="form-control" ng-model="txtRAadhaarNo" maxlength="12" required />
                                                </div>
                                                <span style="color: red" ng-show="regform.rAadhaarNo.$touched && regform.rAadhaarNo.$invalid && regform.rAadhaarNo.$error.required">This is a required field.</span>
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Mobile No.
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px" ng-show="validMobileno">
                                                {{t.MobileNo}}
                                            </div>
                                            <div class="col-md-7 col-sm-7 col-xs-12" style="padding-top: 5px" ng-show="InvalidMobileno">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><b>+91</b></span>
                                                    <input type="text" class="form-control" placeholder="Enter-Mobile Number" name="farmerMobile" ng-model="FarmerMobileNumber" numbers-only mobile-no maxlength="10" required />
                                                </div>
                                                <span style="color: red" ng-show="regform.farmerMobile.$touched && regform.farmerMobile.$invalid && regform.farmerMobile.$error.required">This is a required field.</span>
                                            </div>
                                        </div>
                                    </div>
                                    <div ng-show="validGroup">
                                        <div class="form-group" ng-show="rbApplicantType=='Group'">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Type of Group
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12">
                                                <table>
                                                    <tr ng-repeat="i in grouptype">
                                                        <td>
                                                            <input class="chk" name="chk" ng-click="getValue(i.Code, gType)" type="checkbox" value="{{i.Code}}" id="{{i.Name}}" onchange="togglechk(this.value)" ng-model="gType" required />
                                                            <label style="padding: 0; font-weight: normal" class="control-label">{{i.Name}}</label>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="rbApplicantType=='Group'">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                Firm Name
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-7 col-sm-7 col-xs-12 inputGroupContainer">
                                                <div class="input-group">
                                                    <span class="input-group-addon"><i class="glyphicon glyphicon-home"></i></span>
                                                    <input type="text" alphabet-only name="firmnm" ng-model="txtFirmName" placeholder="Enter the Firm Name" class="form-control" required maxlength="50" />
                                                </div>
                                                <span style="color: red" ng-show="regform.firmnm.$touched && regform.firmnm.$invalid && regform.firmnm.$error.required">This is a required field.</span>
                                            </div>
                                        </div>
                                        <div class="form-group" ng-show="rbApplicantType=='Group'">
                                            <label style="text-align: left" class="control-label col-md-5 col-sm-5 col-xs-12">
                                                No. of Member(s)
                                                <span style="color: red" class="required">*</span>
                                            </label>
                                            <div class="col-md-3 col-sm-3 col-xs-12">
                                                <input type="text" numbers-only hide-zero maxlength="2" name="noofmember" ng-model="txtNoOfMembers" placeholder="Enter the No. of Members" class="form-control" ng-change="initializeTextbox()" required />
                                                <span style="color: red" ng-show="regform.noofmember.$touched && regform.noofmember.$invalid && regform.noofmember.$error.required">This is a required field.</span>
                                            </div>
