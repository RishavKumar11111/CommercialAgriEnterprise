                                <td class="printhide" ng-show="fTextBox">
                                    <label style="font-weight: 100">
                                        <input type="radio" ng-model="rbRC" ng-checked="m.RoadConnectivity == true" value="Yes">
                                        <text>Yes</text>
                                        <input type="radio" ng-model="rbRC" ng-checked="m.RoadConnectivity == false" value="No">
                                        <text>No</text>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Distance from nearby Village</b></td>
                                <td ng-show="fLabel" style="width: 50%">{{m.DistanceFromVillage}} KM</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <input type="text" maxlength="10" valid-number ng-model="txtDFV" placeholder="Enter the Distance from Village" class="form-control" style="width: 55%">
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Electrification</b></td>
                                <td ng-show="fLabel" style="width: 50%">{{m.ElectrificationStatus}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <label style="font-weight: 100">
                                        <input type="radio" ng-model="rbES" ng-checked="m.ElectrificationStatus == true" value="Yes">
                                        <text>Yes</text>
                                        <input type="radio" ng-model="rbES" ng-checked="m.ElectrificationStatus == false" value="No">
                                        <text>No</text>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Pollution Control Clearance</b></td>
                                <td ng-show="fLabel" style="width: 50%">{{m.PollutionControlClearanceStatus}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <label style="font-weight: 100">
                                        <input type="radio" ng-model="rbPCCS" ng-checked="m.PollutionControlClearanceStatus == true" value="Yes">
                                        <text>Yes</text>
                                        <input type="radio" ng-model="rbPCCS" ng-checked="m.PollutionControlClearanceStatus == false" value="No">
                                        <text>No</text>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Self Finance</b></td>
                                <td ng-show="fLabel" style="width: 50%">₹ {{m.SelfFinance}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <input type="text" maxlength="10" valid-number ng-model="txtSF" placeholder="Enter the Self Finance" class="form-control" style="width: 55%">
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Uncensored Loan</b></td>
                                <td ng-show="fLabel" style="width: 50%">₹ {{m.UncensoredLoan}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <input type="text" maxlength="10" valid-number ng-model="txtUL" placeholder="Enter the Uncensored Loan" class="form-control" style="width: 55%">
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Margin Component</b></td>
                                <td ng-show="fLabel" style="width: 50%">₹ {{m.MarginComponent}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <input type="text" maxlength="10" valid-number ng-model="txtMC" placeholder="Enter the Margin Component" class="form-control" style="width: 55%">
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Bank Consent Letter</b></td>
                                <td ng-show="fLabel" style="width: 50%">{{m.BankConsentLetterStatus}}</td>
                                <td class="printhide" ng-show="fTextBox">
                                    <label style="font-weight: 100">
                                        <input type="radio" ng-model="rbBCLS" ng-checked="m.BankConsentLetterStatus == true" value="Yes">
                                        <text>Yes</text>
                                        <input type="radio" ng-model="rbBCLS" ng-checked="m.BankConsentLetterStatus == false" value="No">
                                        <text>No</text>
                                    </label>
                                </td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Photo</b></td>
                                <td style="width: 50%"><img id="landpic" style="width: 250px; height: 150px" /></td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Latitude</b></td>
                                <td style="width: 50%">{{m.Latitude}}</td>
                            </tr>
                            <tr>
                                <td style="width: 50%"><b>Longitude</b></td>
                                <td style="width: 50%">{{m.Longitude}}</td>
                            </tr>
                        </table>
                    </div><br />
                    <p style="text-align: justify; font-size: 15px">Verified the above mentioned information submitted by the entrepreneur by personally visiting the project site. I certify that the <b>{{m.ProjectName}}</b> with <b>{{m.Capacity}} {{m.CapacityUnit}}</b> capacity and a project cost of <b>₹ {{m.TotalProjectCost}}</b> is technically feasible and economically viable.</p><br /><br /><br />
                    <div style="text-align: right">
                        <img id="labelBloSign" style="height: 50px; width: 200px; border: 1px solid black" /><br />
                        <label>{{lblDesignation}}, {{m.BlockName | capitalize}}</label><br />
                        <label>{{labelbloName}}</label>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary" data-dismiss="modal" ng-click="print('printSection')">Print Feasibility Report</button>
                    <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="cancel()">Close</button>
                </div>
            </div>
        </div>
    </div>
    <div id="ApproveModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" id="btnCloseAppM">&times;</button>
                    <h4 class="modal-title">Feasible Record</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group" ng-show="approveDetail">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <label>Do you want to mark the record as feasible?</label>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div ng-show="bloDetail">
                        <div class="form-group">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <label>Name</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <label>{{lblbloName}}</label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <label>Signature</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <img id="bloSign" style="height: 100px; width: 175px; border: 1px solid black" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="modal-footer">
                    <div ng-show="approveDetail">
                        <input type="button" value="Yes" class="btn btn-success" ng-click="approveM()" />
                        <input type="button" value="No" class="btn btn-warning" data-dismiss="modal" />
                    </div>
                    <div ng-show="bloDetail">
                        <input type="button" value="Submit" class="btn btn-info" ng-click="submitBLORecord()" />
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="RejectModal" class="modal fade" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" id="btnCloseRejM">&times;</button>
                    <h4 class="modal-title">Not Feasible Record</h4>
                </div>
                <div class="modal-body">
                    <div class="form-group" ng-show="rejectDetail">
                        <div class="col-md-12 col-sm-12 col-xs-12">
                            <label>Do you want to mark the record as not feasible?</label>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                    <div ng-show="rejectR">
                        <label>Not Feasible Reason</label>
                        <div class="row form-group">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="col-md-1 col-sm-1 col-xs-12">
                                    <label>1.</label>
                                </div>
                                <div class="col-md-11 col-sm-11 col-xs-12">
                                    <textarea rows="2" class="form-control" placeholder="Enter Not Feasible Reason" ng-model="txtReasonOne" ng-trim="false" maxlength="100"></textarea>
                                </div>
                                <p class="text-right">{{100 - txtReasonOne.length + ' characters remaining'}}</p>
