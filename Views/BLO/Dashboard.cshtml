                                    <label style="padding-top: 40px">Name : {{lblRbloName}}</label>
                                </div>
                                <div class="col-md-6 col-sm-6 col-xs-12 text-left">
                                    <img id="bloRSign" style="height: 100px; width: 175px; border: 1px solid black" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="clearfix"></div>
                </div>
                <div class="modal-footer">
                    <div ng-show="rejectDetail">
                        <input type="button" value="Yes" class="btn btn-success" ng-click="rejectM()" />
                        <input type="button" value="No" class="btn btn-warning" data-dismiss="modal" />
                    </div>
                    <div ng-show="bloRDetail">
                        <input type="button" value="Submit" class="btn btn-info" ng-click="notsubmitBLORecord()" />
                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
