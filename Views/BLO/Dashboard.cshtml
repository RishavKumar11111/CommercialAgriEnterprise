@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_LayoutBLO.cshtml";
}
@using (Html.BeginForm("Dashboard", "BLO", FormMethod.Post, new { id = "FormId" }))
{
    @Html.AntiForgeryToken()
}
<div ng-app="BLODApp" ng-controller="BLODCtrl" ng-cloak>
    <div data-loading></div>
    <div class="right_col" role="main">
        <div class="clearfix"></div>
        <div class="row">
            <div class="col-md-12 col-xs-12 col-xs-12">
                <div class="x_panel">
                    <div class="x_title">
                        <h2>Dashboard for Block Level Officer</h2>
                        <div class="clearfix"></div>
                    </div>
                    <div class="x_content">
                        <br />
                        <form class="form-horizontal">
                            <div class="row form-group">
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select ng-model="AaoRefno" ng-options="i.refno for i in getall" ng-change="refnoChange()" class="form-control">
                                        <option value="" disabled style="text-align: center">--Select Reference Number--</option>
                                    </select>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                            </div>
                            <div class="row form-group" ng-show="DROPDOWNLIST">
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <select ng-model="DocDetali" class="form-control" ng-change="dropdownvalue(DocDetali)">
                                        <option value="" disabled style="text-align:center">--Select Details--</option>
                                        <option value="Registration">Registration</option>
                                        <option value="DPR">DPR</option>
                                        <option value="FeasibilityRpt">Feasibility Report</option>
                                    </select>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                            </div>
                            <div class="row form-group" ng-show="approval">
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                                <div class="col-md-6 col-sm-6 col-xs-12">
                                    <button type="button" class="btn btn-info btn-sm" data-toggle="modal" data-target="#ApproveModal" ng-click="openMA()"><span class="glyphicon glyphicon-ok"></span> Feasible</button>
                                    <button type="button" class="btn btn-danger btn-sm" data-toggle="modal" data-target="#RejectModal" ng-click="openMR()"><span class="glyphicon glyphicon-remove"></span> Not Feasible</button>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                            </div>
                            <div class="row form-group" ng-show="notFeasibleView">
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                                <div class="col-md-3 col-sm-3 col-xs-12">
                                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#RModal" ng-click="getRDetails()">View Registration</button>
                                    <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#DModal" ng-click="getDetails()">View DPR</button>
                                </div>
                                <div class="col-md-2 col-sm-2 col-xs-12" ng-show="Integratedproject">
                                    <select class="form-control" ng-model="ddlBLO" ng-change="getDeptCode(ddlBLO)" ng-options="i for i in getDeptOfficer">
                                        <option disabled value="">Select BLO</option>
                                    </select>
                                </div>
                                <div class="col-md-1 col-sm-1 col-xs-12" ng-show="Integratedproject">
                                    <button type="button" class="btn btn-primary" data-toggle="modal" ng-click="SendBLO()">Send</button>
                                </div>
                                <div class="col-md-3 col-sm-3 col-xs-12"></div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div modal="showModal" id="RModal" class="modal fade" role="dialog">
        <div class="modal-dialog modal-lg" style="width: 1300px">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" ng-click="cancel()">&times;</button>
                    <h3 class="modal-title text-center">View & Print Registration</h3>
                </div>
                <div class="modal-body" id="printSection4">
                    <center>
                        <div style="border: 1px solid black; background-color: white; width: 100%">
                            <table style="width: 100%">
                                <tr>
                                    <td style="width: 120px; padding: 10px"><img src="~/Content/images/govtlogo.jpg" /></td>
                                    <td style="text-align: center"><h3>Department of Agriculture and Farmers' Empowerment</h3></td>
                                    <td style="width: 120px; padding: 10px"><img style="height: 80px; width: 86px" src="~/Content/images/ApicolLOGO.jpg" /></td>
                                </tr>
                                <tr>
                                    <td style="text-align: center" colspan="3"><h3>Registration</h3></td>
                                </tr>
                            </table>
                            <hr class="form-group" style="border-top: 1px solid black" />
                            <div class="row form-group">
                                <table style="width: 90%">
                                    <tr>
                                        <td><b>Applicant Details -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                        <td class="printhide" colspan="2">
                                            <input type="button" class="btn btn-xs btn-warning" value="Edit Applicant Details" id="btnEAD" ng-show="!adTextBox" ng-click="showTBHLad()" />
                                            <input type="button" class="btn btn-xs btn-success" value="Update Applicant Details" id="btnUAD" ng-show="!adLabel" ng-click="showLHTBad()" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Reference No.</b></td>
                                        <td>: {{t.ReferenceNumber}}</td>
                                        <td rowspan="8" style="text-align: center"><img id="passpic" style="width: 122px; height: 142px; border: 1px solid black" /><br />Photograph</td>
                                    </tr>
                                    <tr>
                                        <td><b>Date of Application</b></td>
                                        <td>: {{t.DateOfApplication}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Submited to</b></td>
                                        <td>: Block Level Officer : {{t.Block}}, {{t.AppliedTo}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Farmer ID</b></td>
                                        <td>: {{t.FarmerID}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Applicant Type</b></td>
                                        <td>: {{t.ApplicantType}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Applicant Name</b></td>
                                        <td>: {{t.ApplicantName}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Name of the Father / Husband</b></td>
                                        <td>: {{t.FatherorHusbandName}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Gender</b></td>
                                        <td>: {{t.Gender}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Category</b></td>
                                        <td>: {{t.Category}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Educational Qualification</b></td>
                                        <td>: {{t.EducationalQualification}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Annual Income</b></td>
                                        <td ng-show="adLabel">: ₹ {{t.AnnualIncome}}</td>
                                        <td class="printhide" ng-show="adTextBox">
                                            <input type="text" maxlength="15" valid-number id="textAnnualIncome" ng-model="txtAnnualIncome" placeholder="Enter the Annual Income" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Present Occupation</b></td>
                                        <td ng-show="adLabel">: {{t.PresentOccupation}}</td>
                                        <td class="printhide" ng-show="adTextBox">
                                            <input type="text" maxlength="50" alphabet-only id="textPresentOccupation" ng-model="txtPresentOccupation" placeholder="Enter the Present Occupation" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Previous Experience</b></td>
                                        <td ng-show="adLabel">: {{t.PreviousExperience}}</td>
                                        <td class="printhide" ng-show="adTextBox">
                                            <label style="font-weight: 100">
                                                <input type="radio" ng-model="rbPreviousExperience" ng-checked="t.PreviousExperience == true" value="Yes">
                                                <text>Yes</text>
                                                <input type="radio" ng-model="rbPreviousExperience" ng-checked="t.PreviousExperience == false" value="No">
                                                <text>No</text>
                                            </label>
                                        </td>
                                    </tr>
                                    <tr ng-show="t.ApplicantType == 'Group'">
                                        <td><b>Type of Group</b></td>
                                        <td ng-show="adLabel">: {{t.GroupType}}</td>
                                        <td colspan="2" class="printhide" ng-show="adTextBox">
                                            <label style="font-weight: 100" ng-repeat="i in grouptype">
                                                <input type="radio" name="i.Code" ng-model="rbGroupType" ng-click="getCV(i.Code)" ng-checked="t.GroupTypeCode == i.Code" value="{{i.Code}}">
                                                <text>{{i.Name}}</text>
                                            </label>
                                        </td>
                                    </tr>
                                    <tr ng-if="t.ApplicantType == 'Group'">
                                        <td><b>Number of Group Members</b></td>
                                        <td>: {{t.GMNumber}}</td>
                                    </tr>
                                    <tr ng-show="t.ApplicantType == 'Group'">
                                        <td><b>Firm Name</b></td>
                                        <td ng-show="adLabel">: {{t.FirmName}}</td>
                                        <td class="printhide" ng-show="adTextBox">
                                            <input type="text" maxlength="50" alphabet-only id="textFirmName" ng-model="txtFirmName" placeholder="Enter the Present Occupation" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr ng-if="t.ApplicantType == 'Group'">
                                        <td colspan="3"><b>Details of the Group Members -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                    </tr>
                                    <tr ng-if="t.ApplicantType == 'Group'">
                                        <td><b>Sl. No.</b></td>
                                        <td><b>Group Member Farmer ID</b></td>
                                        <td><b>Educational Qualification</b></td>
                                    </tr>
                                    <tr ng-if="t.ApplicantType == 'Group'" ng-repeat="i in grMem">
                                        <td>{{$index+1}}</td>
                                        <td>{{i.GroupMemberFarmerID}}</td>
                                        <td>{{i.EducationalQualificationName}}</td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr>
                                        <td><b>Applicant's Address Details -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                        <td class="printhide">
                                            <input type="button" class="btn btn-xs btn-warning" value="Edit Applicant's Address Details" id="btnEAAD" ng-show="!aadTextBox" ng-click="showTBHLaad()" />
                                            <input type="button" class="btn btn-xs btn-success" value="Update Applicant's Address Details" id="btnUAAD" ng-show="!aadLabel" ng-click="showLHTBaad()" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Correspondance Address (With PIN)</b></td>
                                        <td ng-show="aadLabel">: {{t.CorrespondenceAddress}}</td>
                                        <td class="printhide" ng-show="aadTextBox">
                                            <textarea class="form-control" placeholder="Enter your Address for Correspondence" id="corespondenceAdd" address-only ng-model="txtAddressForCorrespondence" rows="3" ng-trim="false" maxlength="300" style="width: 50%; margin: 5px 0"></textarea>
                                            <p class="text-right">{{300 - txtAddressForCorrespondence.length + ' characters remaining'}}</p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Permanent Address</b></td>
                                        <td ng-show="aadLabel">: {{t.PermanentAddress}}</td>
                                        <td class="printhide" ng-show="aadTextBox">
                                            <textarea class="form-control" placeholder="Enter your Permanent Address" id="permanentAddress" address-only ng-model="txtPermanentAddress" rows="3" ng-trim="false" maxlength="300" style="width: 50%; margin: 5px 0"></textarea>
                                            <p class="text-right">{{300 - txtPermanentAddress.length + ' characters remaining'}}</p>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Pincode</b></td>
                                        <td ng-show="aadLabel">: {{t.Pincode}}</td>
                                        <td class="printhide" ng-show="aadTextBox">
                                            <input type="text" id="Pincode" valid-number hide-zero ng-model="txtPin" placeholder="Enter your Pincode" class="form-control" minlength="6" maxlength="6" style="width: 50%; margin: 5px 0" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Mobile No.</b></td>
                                        <td>: {{t.MobileNumber}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>E-Mail ID</b></td>
                                        <td ng-show="aadLabel">: {{t.EmailID == null ? 'NA' : t.EmailID}}</td>
                                        <td class="printhide" ng-show="aadTextBox">
                                            <input type="email" id="emailID" ng-model="txtEmailID" placeholder="Enter your Email-ID" class="form-control" ng-blur="EmailCheck(txtEmailID)" maxlength="50" style="width: 50%; margin: 5px 0" />
                                        </td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr>
                                        <td><b>Project Informations -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                        <td class="printhide">
                                            <input type="button" class="btn btn-xs btn-warning" value="Edit Project Informations & Costs" id="btnEPI" ng-show="!piTextBox" ng-click="showTBHLpi()" />
                                            <input type="button" class="btn btn-xs btn-success" value="Update Project Informations & Costs" id="btnUPI" ng-show="!piLabel" ng-click="showLHTBpi()" />
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Type of Project</b></td>
                                        <td>: {{t.Department}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Project Name</b></td>
                                        <td>: {{t.Project}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Products / Services to be produced / marketed</b></td>
                                        <td ng-show="piLabel">: {{t.ProductsProduced}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <input type="text" maxlength="50" alphabet-only id="textPPSM" ng-model="txtPPSM" placeholder="Enter the Products / Services" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Capacity</b></td>
                                        <td ng-show="piLabel || t.Capacity == null">: {{t.Capacity == null ? 'NA' : t.Capacity}}</td>
                                        <td class="printhide" ng-show="piTextBox && t.Capacity != null">
                                            <input type="text" maxlength="15" valid-number id="textCapacity" ng-model="txtCapacity" placeholder="Enter the Capacity" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Unit</b></td>
                                        <td>: {{t.CapacityUnit == null ? 'NA' : t.CapacityUnit}}</td>
                                    </tr>
                                    <tr>
                                        <td><b>Means of Finance</b></td>
                                        <td ng-show="piLabel">: {{t.FinanceType}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <label style="font-weight: 100">
                                                <input type="radio" ng-model="rbFinanceType" ng-checked="t.FinanceType == 'Self'" value="Self" ng-click="ifBank = false; updateSum()">
                                                <text>Self</text>
                                                <input type="radio" ng-model="rbFinanceType" ng-checked="t.FinanceType == 'Bank'" value="Bank" ng-click="showTBHLpi(); updateSum()">
                                                <text>Bank</text>
                                            </label>
                                        </td>
                                    </tr>
                                    <tr ng-show="ifBank">
                                        <td><b>Bank Name</b></td>
                                        <td ng-show="piLabel">: {{t.Bank}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <select class="form-control" name="Bank" ng-model="ddlBank" ng-change="getBranch(ddlBank)" ng-options="i.intId as i.vchBankName for i in bank" style="width: 50%; margin: 5px 0">
                                                <option value="" disabled>---Select the Bank---</option>
                                            </select>
                                        </td>
                                    </tr>
                                    <tr ng-show="ifBank">
                                        <td><b>Branch Name</b></td>
                                        <td ng-show="piLabel">: {{t.Branch}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <select class="form-control" name="Branch" ng-model="ddlBranch" ng-options="i.intBranchId as i.vchBranchName for i in branch" style="width: 50%; margin: 5px 0">
                                                <option value="" disabled>---Select the Branch---</option>
                                            </select>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Whether CIS previously availed for any project</b></td>
                                        <td>: {{t.IsCISPreviouslyAvailed}}</td>
                                    </tr>
                                    <tr ng-if="t.IsCISPreviouslyAvailed=='Yes'">
                                        <td><b>CIS Project Type</b></td>
                                        <td>: {{t.CISProject}}</td>
                                    </tr>
                                    <tr ng-if="t.IsCISPreviouslyAvailed=='Yes'">
                                        <td><b>CIS Location</b></td>
                                        <td>: {{t.CISProject}}</td>
                                    </tr>
                                    <tr ng-if="t.IsCISPreviouslyAvailed=='Yes'">
                                        <td><b>CIS Availed Year</b></td>
                                        <td>: {{t.CISProject}}</td>
                                    </tr>
                                    <tr ng-if="t.IsCISPreviouslyAvailed=='Yes'">
                                        <td><b>CIS Finance Type</b></td>
                                        <td>: {{t.CISProject}}</td>
                                    </tr>
                                    <tr ng-if="t.IsCISPreviouslyAvailed=='Yes'">
                                        <td><b>CIS Amount</b></td>
                                        <td>: {{t.CISProject}}</td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr ng-if="t.Project == 'Integrated Farming'">
                                        <td colspan="5"><b>Projects under Integrated Farming -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                    </tr>
                                    <tr ng-if="t.Project == 'Integrated Farming'">
                                        <td><b>Sl. No.</b></td>
                                        <td><b>Projects</b></td>
                                        <td><b>Sectors</b></td>
                                        <td><b>Capacity</b></td>
                                        <td><b>Unit</b></td>
                                    </tr>
                                    <tr ng-if="t.Project == 'Integrated Farming'" ng-repeat="i in iProjects">
                                        <td>{{$index+1}}</td>
                                        <td>{{i.ProjectTypeName}}</td>
                                        <td>{{i.DepartmentName}}</td>
                                        <td>{{i.Capacity == null ? 'NA' : i.Capacity}}</td>
                                        <td>{{i.Unit == null ? 'NA' : i.Unit}}</td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr>
                                        <td colspan="2"><b>Tentative Cost of the Project (in ₹) -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Approximate Cost</b></td>
                                        <td ng-show="piLabel">: {{t.ApproximateCost}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <label style="font-weight: 100">
                                                <input type="radio" ng-model="rbAC" ng-checked="t.FinanceType == 'Below 1 Crore'" value="Below 1 Crore">
                                                <text>Below 1 Crore</text>
                                                <input type="radio" ng-model="rbAC" ng-checked="t.FinanceType == '1 Crore or Above'" value="1 Crore or Above">
                                                <text>1 Crore or Above</text>
                                            </label>
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Own Contribution</b></td>
                                        <td ng-show="piLabel">: ₹ {{t.OwnContribution}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <input type="text" maxlength="12" valid-number hide-zero ng-model="txtOwnContribution" ng-change="updateSum()" placeholder="Enter the Own Contribution" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr ng-show="ifBank">
                                        <td><b>Bank Loan</b></td>
                                        <td ng-show="piLabel">: ₹ {{t.BankLoan}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <input type="text" maxlength="12" valid-number hide-zero ng-model="txtBankLoan" ng-change="updateSum()" placeholder="Enter the Bank Loan" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr>
                                        <td><b>Total Cost</b></td>
                                        <td ng-show="piLabel">: ₹ {{t.TotalCost}}</td>
                                        <td class="printhide" ng-show="piTextBox">
                                            <input type="text" maxlength="15" valid-number hide-zero disabled ng-model="txtTotal" class="form-control" style="width: 50%; margin: 5px 0">
                                        </td>
                                    </tr>
                                    <tr><td>&nbsp;</td><td>&nbsp;</td></tr>
                                    <tr>
                                        <td colspan="2"><b>Documents Attached -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Land Record / Lease Document</b></td>
                                        <td>: <input type="checkbox" disabled checked /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Education Certificate</b></td>
                                        <td>: <input type="checkbox" disabled ng-checked="t.EducationalCertificate != ''" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Bank Consent Letter</b></td>
                                        <td>: <input type="checkbox" disabled ng-checked="t.BankConsentLeter != ''" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>CIS Bank Clearance Certificate</b></td>
                                        <td>: <input type="checkbox" disabled ng-checked="t.CISBankConsentLeter != ''" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Photograph of the Applicant</b></td>
                                        <td>: <input type="checkbox" disabled checked /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Identity Proof</b></td>
                                        <td>: <input type="checkbox" disabled checked /></td>
                                    </tr>
                                </table>
                                <br />
                                <table id="ackTable" style="width: 90%; border: 1px solid black">
                                    <tr>
                                        <td colspan="17" style="text-align: left"><b>Proposed Location of CAE with land Particulars -----</b><br /><input type="image" src="~/Content/images/ul.jpg" /></td>
                                    </tr>
                                    <tr>
                                        <td><b>Sl. No.</b></td>
                                        <td><b>Khata No.</b></td>
                                        <td><b>Plot No.</b></td>
                                        <td><b>District</b></td>
                                        <td ng-if="localRU == 'Rural'"><b>Block</b></td>
                                        <td ng-if="localRU == 'Rural'"><b>GP</b></td>
                                        <td ng-if="localRU == 'Urban'"><b>Tehsil</b></td>
                                        <td ng-if="localRU == 'Urban'"><b>RI Circle</b></td>
                                        <td><b>Village</b></td>
                                        <td><b>Tenant Name</b></td>
                                        <td><b>Kisam</b></td>
                                        <td><b>Area In Hectare</b></td>
                                        <td><b>Area In Acre</b></td>
                                        <td><b>Area of Execution</b></td>
                                        <td><b>Unit of Execution Area</b></td>
                                        <td><b>Relationship</b></td>
                                        <td><b>Locality</b></td>
                                    </tr>
                                    <tr ng-repeat="i in landRecord">
                                        <td>{{$index+1}}</td>
                                        <td>{{i.KhataNo}}</td>
                                        <td>{{i.PlotNo}}</td>
                                        <td>{{i.DistName}}</td>
                                        <td ng-if="i.Locality == 'Rural'">{{i.BlockName}}</td>
                                        <td ng-if="i.Locality == 'Rural'">{{i.GPName}}</td>
                                        <td ng-if="i.Locality == 'Urban'">{{i.TehsilName}}</td>
                                        <td ng-if="i.Locality == 'Urban'">{{i.RICircleName}}</td>
