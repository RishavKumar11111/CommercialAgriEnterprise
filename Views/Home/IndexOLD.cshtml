<body oncontextmenu="return false;" onload="noback();" onpageshow="if (event.persisted) noback();" unload="">
    <div class="box-wrap">
        <header role="banner" id="fh5co-header">
            <div class="container">
                <nav class="navbar navbar-default">
                    <div class="row" style="margin-right: -100px; margin-left: -35px">
                        <div class="col-md-4 col-sm-4 col-xs-4">
                            <div class="fh5co-navbar-brand">
