    <style type="text/css">
        .flip-container {
            margin: 50px;
            perspective: 1000;
            display: inline-block;
        }
        .flip-container:hover .back {
            transform: rotateY(180deg);
        }
        .flip-container:hover .front {
            transform: rotateY(0deg);
        }
        .flip-container, .front, .back {
            width: 200px;
            height: 200px;
        }
        .card {
            transition: 0.6s;
            transform-style: preserve-3d;
            position: relative;
        }
        .front, .back {
            position: absolute;
            backface-visibility: hidden;
            transition: 0.6s;
            transform-style: preserve-3d;
        }
        .front {
            z-index: 1;
            transform: rotateY(-180deg);
            background: url('../../Content/images/pdf.jpg') no-repeat;
        }
        .REGFRONT {
            z-index: 1;
            transform: rotateY(-180deg);
            background: url('../../Content/images/VideoIcon.jpg') no-repeat;
        }
        .flip-container:hover .DPR {
            transform: rotateY(180deg);
        }
        .flip-container, .front, .DPR {
            width: 200px;
            height: 200px;
        }
        .front, .DPR {
            position: absolute;
            backface-visibility: hidden;
            transition: 0.6s;
            transform-style: preserve-3d;
        }
        .DPR {
            z-index: 2;
            transform: rotateY(0deg);
            background: url('../../Content/images/DPRMANUAL.png') no-repeat;
